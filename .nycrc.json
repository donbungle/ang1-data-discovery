{
  "include": [
    "app/modules/**/*.js"
  ],
  "exclude": [
    "app/modules/**/*-test.js",
    "test/**/*",
    "node_modules/**/*",
    "dist/**/*",
    "bower_components/**/*"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output",
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80
}